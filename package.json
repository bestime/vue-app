{
  "name": "vite-vue3-ts-eslint-prettier",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode development",
    "build:hash": "vite build --mode production_hash",
    "build:history": "vite build --mode production_history",
    "lint-fix": "eslint --fix src --ext .ts,.vue,.js,.cjs --ignore-path .gitignore",
    "lint-check": "eslint --no-fix src --ext .ts,.vue,.js,.cjs --ignore-path .gitignore --max-warnings 0",
    "prepare": "husky install"
  },
  "dependencies": {    
    "axios": "^1.3.3",
    "dayjs": "^1.11.7",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "sass": "^1.58.3",
    "@tsconfig/node18": "^2.0.0",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@rushstack/eslint-patch": "1.2.0",
    "@types/node": "20.1.1",
    "@vitejs/plugin-vue": "4.2.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "0.3.2",
    "eslint": "8.40.0",
    "eslint-plugin-vue": "9.11.1",
    "husky": "^8.0.3",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vue-tsc": "1.6.4"
  }
}
