{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "types/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "public/static/extends/jUtilsBase/umd/index.d.ts",
    "public/static/extends/jUtilsBrowser/umd/index.d.ts"
    
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": [
      "ant-design-vue/typings/global.d.ts"
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
